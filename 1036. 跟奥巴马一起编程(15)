#include<stdio.h>
  int main(){
    char a;
    int n;
    scanf("%d %c",&n,&a);
    int n1 = n;
      if (n1 % 2 != 0){
        n1++;
      }
      for (int i = 0;i < n1 / 2;i++){
      if (i == 0||i == (n1 / 2-1)){
        int j = 0;
        while (j++ < n){
          printf("%c",a);
        }printf("\n");
        }else{
          printf("%c",a);
          int p = 1 ;
          while (p++ < n-1){
          printf(" ");
        }
          printf("%c\n",a);
    }
  }
    
    return 0;
 } 
