#include<stdio.h>
#include<string.h>
	int main(){
		int zqd,gr;
		char n;
		scanf("%d %d",&zqd,&gr);
		n = getchar();
		if (gr){
			printf("%d %d",zqd * gr,gr - 1);
		}else{
			if (n == '\n'){
				printf("0 0");
			}		
		}
		while (n != '\n'){
			scanf("%d %d",&zqd,&gr);
			n = getchar();
			if (gr){
			printf(" %d %d",zqd * gr,gr - 1);
			}
		}
		return 0;
	}
