#include<stdio.h>
	int main(){
		int kirby[10] = {0};
		for (int i = 0;i < 10;i++){
			scanf("%d",&kirby[i]);
		}
		for (int i = 1;i < 10;i++){
			if (kirby[i] != 0){
				printf("%d",i);
				kirby[i]--;
				break;
			}
		}for (int i = 0;i < 10;i++){
			while (kirby[i]--){
				printf("%d",i);
			}
		}
		return 0;
	} 
